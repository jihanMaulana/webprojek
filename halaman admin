======================================================FILE CSS:====================================================================


.container{
	margin-bottom: 30px;
	

}

.kiri{
	width: 500px;
	height: 500px;
	float: left;
	border: 3px solid;
	margin-left: 30px;

}

.logo{
	position: static;
	border: 3px solid;
	margin-bottom: 30px;
	text-align: center;

}

.menu{
	position: static;
	border: 3px solid;
	text-align: center;
}
.scroll{
	border: 3px solid;
	height: 330px;
	overflow: scroll;
}

.tengah{
	border: 3px solid;
	width: 50%;
	padding: 30px;
	float: left;
	margin-left: 20px;


}



.berhenti{
	clear: left;
}

td{
	text-align: center;
}

.footer{
	position: static;
	border: 3px solid;
	background-color: black;
	margin-left: 20px;


}

.footer h2{
	text-decoration: none;
	text-align: center;
	margin: auto;
	color: white;


}


/**
 * For modern browsers
 * 1. The space content is one way to avoid an Opera bug when the
 *    contenteditable attribute is included anywhere else in the document.
 *    Otherwise it causes space to appear at the top and bottom of elements
 *    that are clearfixed.
 * 2. The use of `table` rather than `block` is only necessary if using
 *    `:before` to contain the top-margins of child elements.
 */
.cf:before,
.cf:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.cf:after {
    clear: both;
}

/**
 * For IE 6/7 only
 * Include this rule to trigger hasLayout and contain floats.
 */
.cf {
    *zoom: 1;
}
